{
  "name": "veggies",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only -- {@}\" -- --mode staging",
    "build:production": "run-p type-check \"build-only -- {@}\" -- --base=/",
    "preview": "vite preview",
    "test:unit": "vitest",
