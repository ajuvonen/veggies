    "@xstate/vue": "^4.0.0",
    "canvas-confetti": "^1.9.3",
    "chart.js": "^4.4.6",
    "chartjs-plugin-datalabels": "^2.2.0",
    "luxon": "^3.5.0",
    "pinia": "^2.2.6",
    "remeda": "^2.17.4",
    "simple-icons": "^13.18.0",
    "vue": "3.5.13",
    "vue-chartjs": "^5.3.2",
