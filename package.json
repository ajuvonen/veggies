    "@xstate/vue": "^3.1.4",
    "canvas-confetti": "^1.9.3",
    "chart.js": "^4.4.5",
    "chartjs-plugin-datalabels": "^2.2.0",
    "luxon": "^3.5.0",
    "pinia": "^2.2.4",
    "remeda": "^2.16.0",
    "vue": "3.5.12",
    "vue-chartjs": "^5.3.1",
    "vue-i18n": "^10.0.4",
    "vue-router": "^4.4.5",
    "xstate": "^5.18.2"
